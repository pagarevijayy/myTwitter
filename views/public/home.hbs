    <link rel="icon" href="./img/favicon.ico">
    <title>Home / myTwitter</title>
</head>

<body>

    <div class="container">
        <div class="row">
            {{!-- leftPanel has col-3 --}}
            {{>leftPanel}}

            <div class="col-9">
                {{!-- Top notation --}}
                <div class="mt-3 border-bottom sticky-top container">
                    <a class="link-rm-defaults font-weight-bolder text-body" href="#">
                        <h5>Home</h5>
                    </a>
                </div>
                {{!-- Feed --}}
                {{>tweetsAndRetweets}}
